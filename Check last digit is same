#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int claculate(int num1, int num2) {
    int num3 = num1-((num1/10)*10);
    int num4 = num2-((num2/10)*10);
    if ( num3 == num4 ) {
        return 1;
    }
    else {
        return 2;
    }
}
int main() {
    int num1_input,num2_input;
    scanf("%d %d",&num1_input,&num2_input);
    
    int value = claculate(num1_input, num2_input);
    
    if (value == 1) {
        printf("YES");
    }
    else {
    printf("NO");
    }
    return 0;
}
