#include <stdio.h>

int main() {

   int h, t;
    float c;
    int cond1, cond2, cond3;
    int grade;


    scanf("%d %f %d", &h, &c, &t);


    cond1 = (h > 50);
    cond2 = (c < 0.7);
    cond3 = (t > 5600);


    if (cond1 && cond2 && cond3) {
        grade = 10;
    }
    else if (cond1 && cond2) {
        grade = 9;
    }
    else if (cond2 && cond3) {
        grade = 8;
    }
    else if (cond1 && cond3) {
        grade = 7;
    }
    else if (cond1 || cond2 || cond3 ) {
        grade = 6;
    }
    else {
        grade = 5;
    }
       
    printf("The grade of the steel is: %d\n",grade);
    
    
    if (grade==10){
        
        printf("All of the conditions met.");
    }
    
         else if (grade==9){
          
            printf("Two conditions met.");
         }
    
         else if ( grade==8){
           printf("Two conditions met.");
         }
    
         else if ( grade==7){
           printf("Two conditions met.");
         }
    
         else if ( grade==6){
            printf("Only one condition met.");
         }
    
    
    else{
        printf("None of the conditions met.");
        
    }
    return 0;
}
